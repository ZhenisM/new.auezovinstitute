43b\u0438\u043a\u043e\u0439 \u0441\u0442\u0435\u043f\u0438, \u0420\u043e\u0434\u0438\u043d\u044b, \u043c\u0430\u043b\u043e\u0439 \u0420\u043e\u0434\u0438\u043d\u044b, \u0430\u0440\u0445\u0435\u0442\u0438\u043f \u0414\u043e\u043c\u0430 \u2013 \u0432\u0435\u0434\u0443\u0449\u0438\u0435 \u0432 \u0441\u0442\u0438\u0445\u043e\u0442\u0432\u043e\u0440\u0435\u043d\u0438\u044f\u0445 \u0438 \u043f\u043e\u044d\u043c\u0430\u0445 \u0438\u0437\u0432\u0435\u0441\u0442\u043d\u044b\u0445 \u043f\u043e\u044d\u0442\u043e\u0432 \u041e\u043b\u0436\u0430\u0441\u0430 \u0421\u0443\u043b\u0435\u0439\u043c\u0435\u043d\u043e\u0432\u0430, \u0423\u043b\u0443\u0433\u0431\u0435\u043a\u0430 \u0415\u0441\u0434\u0430\u0443\u043b\u0435\u0442\u0430, \u041d\u0443\u0440\u043b\u0430\u043d\u0430 \u041e\u0440\u0430\u0437\u0430\u043b\u0438\u043d\u0430, \u0428\u043e\u043c\u0438\u0448\u0431\u0430\u044f \u0421\u0430\u0440\u0438\u0435\u0432\u0430, \u0411\u0430\u0443\u0440\u0436\u0430\u043d\u0430 \u0416\u0430\u043a\u044b\u043f\u0430, \u0411\u0430\u0445\u044b\u0442\u0430 \u041a\u0430\u0438\u0440\u0431\u0435\u043a\u043e\u0432\u0430, \u0411\u0430\u0445\u044b\u0442\u0436\u0430\u043d\u0430 \u041a\u0430\u043d\u0430\u043f\u044c\u044f\u043d\u043e\u0432\u0430, \u0415\u0441\u0435\u043d\u0433\u0430\u043b\u0438 \u0420\u0430\u0443\u0448\u0430\u043d\u043e\u0432\u0430, \u0413\u0430\u043b\u044b\u043c\u0430 \u0416\u0430\u0439\u043b\u044b\u0431\u0430\u044f, \u041c\u0430\u0440\u0444\u0443\u0433\u0438 \u0410\u0439\u0442\u0445\u043e\u0436\u0438\u043d\u043e\u0439, \u041d\u0430\u0434\u0435\u0436\u0434\u044b \u0427\u0435\u0440\u043d\u043e\u0432\u043e\u0439, \u0412\u0430\u043b\u0435\u0440\u0438\u044f \u041c\u0438\u0445\u0430\u0439\u043b\u043e\u0432\u0430, \u0422\u0430\u0442\u044c\u044f\u043d\u044b \u0424\u0440\u043e\u043b\u043e\u0432\u0441\u043a\u043e\u0439, \u041a\u0430\u0439\u0440\u0430\u0442\u0430 \u0411\u0430\u043a\u0431\u0435\u0440\u0433\u0435\u043d\u043e\u0432\u0430, \u041e\u0440\u044b\u043d\u0431\u0430\u044f \u0416\u0430\u043d\u0430\u0439\u0434\u0430\u0440\u043e\u0432\u0430, \u041c\u0430\u0440\u0430\u043b\u0442\u0430\u044f \u042b\u0431\u044b\u0440\u0430\u0435\u0432\u0430, \u0410\u043a\u0431\u0435\u0440\u0435\u043d\u0430 \u0415\u043b\u0433\u0435\u0437\u0435\u043a\u0430, \u0413\u0440\u0438\u0444\u0430 \u0425\u0430\u0439\u0440\u0443\u043b\u043b\u0438\u043d\u0430, \u041b\u044e\u0431\u043e\u0432\u0438 \u0428\u0430\u0448\u043a\u043e\u0432\u043e\u0439, \u041e\u043b\u044c\u0433\u0438 \u0413\u0440\u0438\u0433\u043e\u0440\u044c\u0435\u0432\u043e\u0439, \u0420\u0438\u043c\u043c\u044b \u0410\u0440\u0442\u0435\u043c\u044c\u0435\u0432\u043e\u0439, \u0421\u0442\u0430\u043d\u0438\u0441\u043b\u0430\u0432\u0430 \u041b\u0438, , \u0424\u0430\u0440\u0445\u0430\u0442\u0430 \u0422\u0430\u043c\u0435\u043d\u0434\u0430\u0440\u043e\u0432\u0430, \u0410\u0431\u0434\u0438\u0440\u0430\u0445\u0438\u043c\u0430 \u041f\u0440\u0430\u0442\u043e\u0432\u0430, \u0411\u0430\u0432\u0435 \u041d\u0430\u0437\u0438\u0440\u0430, \u0421\u0430\u043b\u0435\u0445\u0430 \u0421\u0430\u044f\u0434\u0438, \u041c\u0435\u0434\u0436\u0438\u0442\u0435 \u0421\u0443\u043b\u0435\u0439\u043c\u0430\u043d\u0430,  \u0421\u0430\u044f\u0442\u0430 \u041a\u0430\u043c\u0448\u0438\u0433\u0435\u0440\u0430, \u041f\u0430\u0442\u0438\u0433\u0443\u043b\u044c \u041c\u0430\u043a\u0441\u0430\u0442\u043e\u0432\u043e\u0439, \u0417\u0430\u0440\u044b \u0411\u0430\u0434\u043e\u0435\u0432\u043e\u0439, \u0417\u0443\u043b\u043f\u044b \u0427\u0443\u043c\u0430\u043a\u043e\u0432\u043e\u0439, \u041b\u044e\u0434\u043c\u0438\u043b\u044b \u0425\u043e\u0447\u0438\u0435\u0432\u043e\u0439, \u0418\u0431\u0440\u043e\u0445\u0438\u043c\u0430 \u0422\u0443\u0440\u043a\u0438, \u0420\u0430\u0443\u0448\u0430\u043d \u0411\u0443\u0440\u043a\u0438\u0442\u0431\u0430\u0435\u0432\u043e\u0439-\u041d\u0443\u043a\u0435\u043d\u043e\u0432\u043e\u0439, \u0413\u0443\u043b\u044c\u043d\u0430\u0440\u044b \u0410\u0443\u0442\u043e\u0432\u043e\u0439, \u0410\u043b\u0435\u0441\u044f \u0427\u0435\u0440\u043a\u0430\u0441\u043e\u0432\u0430, \u041c\u0438\u0440\u043f\u043e\u043b\u0430\u0442 \u0413\u0443\u043b\u0438, \u0414\u043c\u0438\u0442\u0440\u0438\u044f \u0412\u0430\u0439\u0441\u0431\u0435\u043a\u043a\u0435\u0440-\u0418\u0432\u0430\u043d\u043e\u0432\u0430, \u0420\u043e\u043c\u0430\u043d\u0430 \u0416\u0443\u043a\u043e\u0432\u0430 \u0438 \u043c\u043d\u043e\u0433\u0438\u0445 \u0434\u0440\u0443\u0433\u0438\u0445.\n\u0412 \u0437\u0430\u0432\u0435\u0440\u0448\u0438\u0432\u0448\u0438\u0439\u0441\u044f \u0433\u043e\u0434 75-\u043b\u0435\u0442\u0438\u044f \u0412\u0435\u043b\u0438\u043a\u043e\u0439 \u041f\u043e\u0431\u0435\u0434\u044b \u0432\u043e \u0432\u0442\u043e\u0440\u043e\u043c \u0442\u043e\u043c\u0435 \u0445\u0440\u0435\u0441\u0442\u043e\u043c\u0430\u0442\u0438\u0438 \u00ab\u041a\u0430\u0437\u0430\u0445\u0441\u0442\u0430\u043d \u0432 \u043c\u043e\u0435\u0439 \u0441\u0443\u0434\u044c\u0431\u0435\u00bb \u043e\u043f\u0443\u0431\u043b\u0438\u043a\u043e\u0432\u0430\u043d\u044b \u043f\u0440\u043e\u0438\u0437\u0432\u0435\u0434\u0435\u043d\u0438\u044f \u0432\u043e\u0435\u043d\u043d\u043e\u0439 \u0442\u0435\u043c\u0430\u0442\u0438\u043a\u0438, \u043d\u0430\u043f\u0440\u0430\u0432\u043b\u0435\u043d\u043d\u044b\u0435 \u043d\u0430 \u0432\u043e\u0441\u043f\u0438\u0442\u0430\u043d\u0438\u0435 \u043a\u0430\u0437\u0430\u0445\u0441\u0442\u0430\u043d\u0441\u043a\u043e\u0433\u043e \u043f\u0430\u0442\u0440\u0438\u043e\u0442\u0438\u0437\u043c\u0430 \u0438 \u0443\u043a\u0440\u0435\u043f\u043b\u0435\u043d\u0438\u0435 \u044d\u0442\u043d\u043e\u043d\u0430\u0446\u0438\u043e\u043d\u0430\u043b\u044c\u043d\u043e\u0439 \u0438\u0434\u0435\u043d\u0442\u0438\u0447\u043d\u043e\u0441\u0442\u0438. \n\u041f\u0440\u043e\u0434\u043e\u043b\u0436\u0435\u043d\u0438\u0435 \u2b07\ufe0f\u2b07\ufe0f\u2b07\ufe0f","link":"https:\/\/www.instagram.com\/p\/CNQAemhAker","type":"image","fullsize":"https:\/\/scontent-frt3-1.cdninstagram.com\/v\/t51.2885-15\/e35\/s1080x1080\/167927544_276053090686099_6919091306454642583_n.jpg?tp=1&_nc_