3 \u0442\u0430\u04a3\u0434\u0430\u043c\u0430\u043b\u044b \u043d\u04b1\u0441\u049b\u0430\u043b\u0430\u0440\u044b \u0435\u043d\u0433\u0456\u0437\u0456\u043b\u0434\u0456.\n\ud83d\udd39 \u0425\u0430\u043b\u044b\u049b \u04e9\u043b\u0435\u04a3\u0434\u0435\u0440\u0456\u043d \u0435\u043b \u0430\u0440\u0430\u0441\u044b\u043d\u0430\u043d \u0436\u0438\u043d\u0430\u0443, \u0436\u0430\u0440\u0438\u044f\u043b\u0430\u0443, \u04e9\u0437\u0433\u0435 \u0442\u0456\u043b\u0434\u0435\u0440\u0433\u0435 \u0430\u0443\u0434\u0430\u0440\u0443 \u0436\u04b1\u043c\u044b\u0441\u0442\u0430\u0440\u044b \u0425\u0406\u0425 \u0493\u0430\u0441\u044b\u0440\u0434\u044b\u04a3 \u0435\u043a\u0456\u043d\u0448\u0456 \u0436\u0430\u0440\u0442\u044b\u0441\u044b\u043d\u0430\u043d \u0431\u0430\u0441\u0442\u0430\u0443 \u0430\u043b\u0430\u0434\u044b. \u041e\u0440\u044b\u043d\u0431\u043e\u0440 \u049b\u0430\u043b\u0430\u0441\u044b\u043d\u0434\u0430 \u0422.\u0410.\u0421\u0435\u0439\u0434\u0430\u043b\u0438\u043d \u043c\u0435\u043d \u0421.\u0410.\u0416\u0430\u043d\u0442\u04e9\u0440\u0438\u043d \u0436\u0438\u043d\u0430\u043f, \u049b\u04b1\u0440\u0430\u0441\u0442\u044b\u0440\u0493\u0430\u043d \u00ab\u041e\u0431\u0440\u0430\u0437\u0446\u044b \u043a\u0438\u0440\u0433\u0438\u0437\u0441\u043a\u043e\u0439 \u043f\u043e\u044d\u0437\u0438\u0438\u00bb \u0430\u0442\u0442\u044b \u043a\u0456\u0442\u0430\u043f \u0430\u043b\u0493\u0430\u0448 \u0440\u0435\u0442 1872 \u0436\u044b\u043b\u044b \u0436\u0430\u0440\u044b\u049b\u049b\u0430 \u0448\u044b\u049b\u0442\u044b.\n\ud83d\udd39 \u0425\u0425 \u0493\u0430\u0441\u044b\u0440\u0434\u044b\u04a3 \u0431\u0430\u0441\u044b\u043d\u0434\u0430 \u0421.\u0421\u0435\u0439\u0444\u0443\u043b\u043b\u0438\u043d, \u0406.\u0416\u0430\u043d\u0441\u04af\u0433\u0456\u0440\u04b1\u043b\u044b, \u0421.\u041c\u04b1\u049b\u0430\u043d\u043e\u0432, \u041c.\u04d8\u0443\u0435\u0437\u043e\u0432, \u0410.\u0411\u0430\u0439\u0442\u04b1\u0440\u0441\u044b\u043d\u04b1\u043b\u044b, \u0411.\u0423\u0430\u0445\u0430\u0442\u043e\u0432, \u041d.\u0422\u04e9\u0440\u0435\u049b\u04b1\u043b\u043e\u0432, \u049a.\u0421\u04d9\u0442\u0431\u0430\u0435\u0432, \u04d8.\u041c\u0430\u0440\u0493\u04b1\u043b\u0430\u043d, \u049a.\u049a\u0443\u0430\u043d\u044b\u0448\u0431\u0430\u0435\u0432, \u04d8.\u0416\u0430\u043d\u0433\u0435\u043b\u0434\u0438\u043d, \u0415.\u04e8\u043c\u0456\u0440\u0437\u0430\u049b\u043e\u0432, \u0415.\u042b\u0441\u043c\u0430\u0439\u044b\u043b\u043e\u0432, \u049a.\u0416\u0430\u043d\u0434\u0430\u0440\u0431\u0435\u043a\u043e\u0432, \u0492.\u049a\u04b1\u0440\u043c\u0430\u043d\u0493\u0430\u043b\u0438\u0435\u0432, \u04d8.\u049a\u0430\u0448\u0430\u0443\u0431\u0430\u0435\u0432, \u0421.\u0411\u0435\u0433\u0430\u043b\u0438\u043d, \u04e8.\u0422\u04b1\u0440\u043c\u0430\u043d\u0436\u0430\u043d\u043e\u0432 \u0441\u0438\u044f\u049b\u0442\u044b \u0437\u0438\u044f\u043b\u044b \u049b\u0430\u0443\u044b\u043c \u04e9\u043a\u0456\u043b\u0434\u0435\u0440\u0456 \u0445\u0430\u043b\u044b\u049b \u04e9\u043b\u0435\u04a3\u0434\u0435\u0440\u0456\u043d\u0456\u04a3 \u0436\u0438\u043d\u0430\u043b\u0443\u044b\u043d\u0430, \u0436\u0430\u0440\u0438\u044f\u043b\u0430\u043d\u0443\u044b\u043d\u0430 \u0431\u0435\u043b\u0441\u0435\u043d\u0434\u0456 \u0430\u0440\u0430\u043b\u0430\u0441\u0442\u044b. 20-30 \u0436\u044b\u043b\u0434\u0430\u0440\u044b \u04d8.\u0414\u0438\u0432\u0430\u0435\u0432, \u0410.\u0417\u0430\u0442\u0430\u0435\u0432\u0438\u0447, \u0406.\u0416\u0430\u043d\u0441\u04af\u0433\u0456\u0440\u04b1\u043b\u044b \u04b1\u0439\u044b\u043c\u0434\u0430\u0441\u0442\u044b\u0440\u0493\u0430\u043d \u0430\u0440\u043d\u0430\u0439\u044b \u044d\u043a\u0441\u043f\u0435\u0434\u0438\u0446\u0438\u044f\u043b\u0430\u0440\u0434\u044b\u04a3 \u043d\u0435\u0433\u0456\u0437\u0456\u043d\u0434\u0435 \u0445\u0430\u043b\u044b\u049b \u04e9\u043b\u0435\u04a3\u0434\u0435\u0440\u0456\u043d\u0456\u04a3 \u049b\u043e\u0440\u044b \u0436\u0438\u043d\u0430\u049b\u0442\u0430\u043b\u044b\u043f, \u04d9\u0443\u0435\u043d\u0434\u0435\u0440\u0456 \u043d\u043e\u0442\u0430\u0493\u0430 \u0442\u04af\u0441\u0456\u0440\u0456\u043b\u0434\u0456. \u00ab\u04e8\u0442\u0456\u0440\u0456\u043a\u00bb, \u00ab\u049a\u044b\u0440\u044b\u049b \u04e9\u0442\u0456\u0440\u0456\u043a\u00bb, \u00ab\u049a\u0430\u0437\u0430\u049b\u0442\u044b\u04a3 1000 \u04d9\u043d\u0456\u00bb, \u00ab\u049a\u0430\u0437\u0430\u049b\u0442\u044b\u04a3 500 \u04d9\u043d\u0456 \u043c\u0435\u043d \u043a\u04af\u0439\u0456\u00bb \u0441\u0438\u044f\u049b\u0442\u044b \u0436\u0438\u043d\u0430\u049b\u0442\u0430\u0440 \u0431\u0430\u0441\u044b\u043b\u044b\u043c \u043a\u04e9\u0440\u0434\u0456.\n\ud83d\udd39 \u049a\u04b1\u0440\u0430\u0441\u0442\u044b\u0440\u0443\u0448\u044b\u043b\u0430\u0440\u0434\u044b\u04a3, \u0435\u043b\u0456\u043c\u0456\u0437\u0434\u0435\u0433\u0456 \u0436\u0435\u0442\u0435\u043a\u0448\u0456 \u0444\u043e\u043b\u044c\u043a\u043b\u043e\u0440\u0442\u0430\u043d\u0443\u0448\u044b\u043b\u0430\u0440\u0434\u044b\u04a3, \u0493\u044b\u043b\u044b\u043c\u0438 \u0441\u0430\u0440\u0430\u043f\u0448\u044b\u043b\u0430\u0440\u0434\u044b\u04a3, \u0418\u043d\u0441\u0442\u0438\u0442\u0443\u0442\u0442\u044b\u04a3 \u0493\u044b\u043b\u044b\u043c\u0438 \u043a\u0435\u04a3\u0435\u0441\u0456\u043d\u0456\u04a3 \u04b1\u0441\u044b\u043d\u044b\u0441\u0442\u0430\u0440\u044b \u043d\u04d9\u0442\u0438\u0436\u0435\u0441\u0456\u043d\u0434\u0435 \u0442\u043e\u043c\u043d\u044b\u04a3 \u049b\u04b1\u0440\u044b\u043b\u044b\u043c\u044b \u0430\u043d\u044b\u049b\u0442\u0430\u043b\u044b\u043f, \u049b\u0430\u0440\u0430 \u04e9\u043b\u0435\u04a3, \u0442\u0430\u0440\u0438\u0445\u0438 \u04e9\u043b\u0435\u04a3, \u0442\u043e\u043b\u0493\u0430\u0443, \u0442\u0435\u0440\u043c\u0435, \u04d9\u043d \u04e9\u043b\u0435\u04a3, \u0445\u0430\u0442 \u04e9\u043b\u0435\u04a3, \u043c\u044b\u0441\u0430\u043b \u04e9\u043b\u0435\u04a3, \u04e9\u0442\u0456\u0440\u0456\u043a \u04e9\u043b\u0435\u04a3, \u043c\u044b\u0441\u0430\u043b \u0430\u0439\u0442\u044b\u0441, \u049b\u044b\u0437 \u0431\u0435\u043d \u0436\u0456\u0433\u0456\u0442 \u0430\u0439\u0442\u044b\u0441\u044b\u043d\u044b\u04a3 \u0442\u0430\u04a3\u0434\u0430\u0443\u043b\u044b \u04af\u043b\u0433\u0456\u043b\u0435\u0440\u0456 \u0435\u043d\u0434\u0456.\n\ud83d\udd39 \u0422\u043e\u043c\u043d\u044b\u04a3 \u049b\u04b1\u0440\u044b\u043b\u044b\u043c\u0434\u044b\u049b \u0436\u04af\u0439\u0435\u0441\u0456\u043d\u0435 \u0444\u043e\u043b\u044c\u043a\u043b\u043e\u0440\u043b\u044b\u049b \u0436\u0430\u043d\u0440\u043b\u0430\u0440\u0434\u044b\u04a3 \u049b\u044b\u0441\u049b\u0430\u0448\u0430 \u0430\u043d\u044b\u049b\u0442\u0430\u043c\u0430\u0441\u044b, \u0438\u043b\u043b\u044e\u0441\u0442\u0440\u0430\u0446\u0438\u044f\u043b\u044b\u049b \u0441\u0443\u0440\u0435\u0442\u0442\u0435\u0440, \u043d\u043e\u0442\u0430\u043b\u0430\u0440, \u044d\u0442\u043d\u043e\u0433\u0440\u0430\u0444\u0438\u043b\u044b\u049b \u0441\u0443\u0440\u0435\u0442\u0442\u0456 \u0441\u04e9\u0437\u0434\u0456\u043a, \u043f\u0430\u0439\u0434\u0430\u043b\u0430\u043d\u044b\u043b\u0493\u0430\u043d \u04d9\u0434\u0435\u0431\u0438\u0435\u0442\u0442\u0435\u0440, \u0442\u04af\u0439\u0456\u043d\u0434\u0435\u043c\u0435 (\u043e\u0440\u044b\u0441, \u0430\u0493\u044b\u043b\u0448\u044b\u043d \u0442\u0456\u043b\u0434\u0435\u0440\u0456\u043d\u0434\u0435) \u0441\u0438\u044f\u049b\u0442\u044b \u049b\u043e\u0441\u044b\u043c\u0448\u0430\u043b\u0430\u0440 \u0435\u043d\u0434\u0456.\n\ud83d\udd39 \u041c\u04d9\u0442\u0456\u043d\u0434\u0435\u0440\u0434\u0435 \u043a\u0435\u0437\u0434\u0435\u0441\u0435\u0442\u0456\u043d \u043a\u04e9\u043d\u0435, \u0442\u04af\u0440\u043a\u0456, \u0436\u0435\u0440\u0433\u0456\u043b\u0456\u043a\u0442\u0456, \u043a\u0456\u0440\u043c\u0435 (\u0430\u0440\u0430\u0431, \u043f\u0430\u0440\u0441\u044b, \u043e\u0440\u044b\u0441, \u0442.\u0431.) \u0441\u04e9\u0437\u0434\u0435\u0440\u0433\u0435 \u0442\u04af\u0441\u0456\u043d\u0456\u043a \u0431\u0435\u0440\u0456\u043b\u0434\u0456.\n\ud83d\udd39 \u0418\u043b\u043b\u044e\u0441\u0442\u0440\u0430\u0446\u0438\u044f\u043b\u044b\u049b \u04d9\u0440\u043b\u0435\u0443 \u0436\u04b1\u043c\u044b\u0441\u0442\u0430\u0440\u044b\u043d\u0430 \u04d8.\u049a\u0430\u0441\u0442\u0435\u0435\u0432 \u0430\u0442\u044b\u043d\u0434\u0430\u0493\u044b \u041c\u0435\u043c\u043b\u0435\u043a\u0435\u0442\u0442\u0456\u043a \u04e9\u043d\u0435\u0440 \u043c\u0443\u0437\u0435\u0439\u0456\u043d\u0456\u04a3 \u049b\u043e\u0440\u044b\u043d\u0434\u0430\u0493\u044b \u0441\u0443\u0440\u0435\u0442\u0442\u0435\u0440 \u043f\u0430\u0439\u0434\u0430\u043b\u0430\u043d\u044b\u043b\u0434\u044b.\n\ud83d\udd39 \u0422\u043e\u043c\u043d\u044b\u04a3 \u043a\u0456\u0442\u0430\u0431\u0438 \u0431\u0430\u0441\u044b\u043b\u044b\u043c\u044b\u043c\u0435\u043d \u049b\u0430\u0442\u0430\u0440 \u044d\u043b\u0435\u043a\u0442\u0440\u043e\u043d\u0434\u044b, \u0430\u0443\u0434\u0438\u043e \u043d\u04b1\u0441\u049b\u0430\u043b\u0430\u0440\u044b \u049b\u0430\u0442\u0430\u0440 \u04d9\u0437\u0456\u0440\u043b\u0435\u043d\u0456\u043f, \u0442\u0430\u043d\u044b\u043c\u0430\u043b \u043f\u043e\u0440\u0442\u0430\u043b\u0434\u0430\u0440 \u043c\u0435\u043d \u0441\u0430\u0439\u0442\u0442\u0430\u0440\u0493\u0430 \u0436\u04af\u043a\u0442\u0435\u043b\u0434\u0456 \u0436\u04d9\u043d\u0435 \u043e\u043b\u0430\u0440\u0434\u044b\u04a3 QR \u043a\u043e\u0434\u044b \u043a\u0456\u0442\u0430\u043f \u043c\u04b1\u049b\u0430\u0431\u0430\u0441\u044b\u043d\u0434\u0430 \u043a\u04e9\u0440\u0441\u0435\u0442\u0456\u043b\u0434\u0456.\n\ud83d\udd39 \u0410\u0443\u0434\u0438\u043e \u043d\u04b1\u0441\u049b\u0430\u043b\u0430\u0440\u0434\u044b\u04a3 \u043e\u049b\u044b\u043b\u0443\u044b\u043d\u0430 \u0435\u043b\u0456\u043c\u0456\u0437\u0433\u0435 \u0431\u0435\u043b\u0433\u0456\u043b\u0456 \u0434\u0438\u043a\u0442\u043e\u0440\u043b\u0430\u0440 \u043c\u0435\u043d \u0430\u043a\u0442\u0435\u0440\u043b\u0430\u0440 \u0442\u0430\u0440\u0442\u044b\u043b\u044b\u043f, \u0442\u043e\u043b\u0493\u0430\u0443, \u0442\u0435\u0440\u043c\u0435\u043b\u0435\u0440\u0434\u0456\u04a3, \u0442\u0430\u0440\u0438\u0445\u0438, \u04d9\u043d \u04e9\u043b\u0435\u04a3\u0434\u0435\u0440\u0456\u043d\u0456\u04a3 \u043e\u0440\u044b\u043d\u0434\u0430\u043b\u0443\u044b\u043d\u0430 \u0434\u04d9\u0441\u0442\u04af\u0440\u043b\u0456 \u04e9\u043d\u0435\u0440 \u043c\u0435\u043a\u0442\u0435\u043f\u0442\u0435\u0440\u0456\u043d\u0456\u04a3 \u0442\u0430\u043d\u044b\u043c\u0430\u043b \u04e9\u043a\u0456\u043b\u0434\u0435\u0440\u0456\u043d\u0456\u04a3 \u049b\u0430\u0442\u044b\u0441\u0443\u044b\u043c\u0435\u043d \u0430\u0440\u043d\u0430\u0439\u044b \u0441\u0442\u0443\u0434\u0438\u044f\u0434\u0430 \u0436\u0430\u0437\u044b\u043b\u0434\u044b.","link":"https:\/\/www.instagram.com\/p\/CMb45WDglBI","type":"image","fullsize":"https:\/\/scontent-frt3-1.cdninstagram.com\/v\/t51.2885-15\/e35\/p1080x1080\/160338635_1116875072110096_8912286824979033190_n.jpg?tp=1&_nc_ht=scontent-frt3-1.cdninstagram.com&_nc_cat=104&_nc_ohc=AmVfwROHNf0AX_E191s&edm=APU89FAAAAAA&ccb=7-4&oh=182e90071dba78665b95a9ed93ee9044&oe=609A9A9A&_nc_sid=86f79a","large":"https:\/\/scontent-frt3-1.cdninstagram.com\/v\/t51.2885-15\/sh0.08\/e35\/c0.160.1280.1280a\/s640x640\/160338635_1116875072110096_8912286824979033190_n.jpg?tp=1&_nc_ht=scontent-frt3-1.cdninstagram.com&_nc_cat=104&_nc_ohc=AmVfwROHNf0AX_E191s&edm=APU89FAAAAAA&ccb=7-4&oh=1bb6906ec529e66b80c8bba9f6d5ea15&oe=609AD0D7&_nc_sid=86f79a","small":"https:\/\/scontent-frt3-1.cdninstagram.com\/v\/t51.2885-15\/e35\/c0.160.1280.1280a\/s320x320\/160338635_1116875072110096_8912286824979033190_n.jpg?tp=1&_nc_ht=scontent-frt3-1.cdninstagram.com&_nc_cat=104&_nc_ohc=AmVfwROHNf0AX_E191s&edm=APU89FAAAAAA&ccb=7-4&oh=2ca2407ecbaacb7a5c4619ffcbc84e45&oe=6099786F&_nc_sid=86f79a","likesCount":14,"commentsCount":2,"authorId":4766569164},{"id":"2527559910443506991","code":"CMTsljSgN0v","created":1615528642,"text":"\u049a\u04b1\u0440\u043c\u0435\u0442\u0442\u0456 \u0434\u043e\u0441\u0442\u0430\u0440!\n\u041c. \u041e. \u04d8\u0443\u0435\u0437\u043e\u0432 \u0430\u0442\u044b\u043d\u0434\u0430\u0493\u044b \u04d8\u0434\u0435\u0431\u0438\u0435\u0442 \u0436\u04d9\u043d\u0435 \u04e9\u043d\u0435\u0440 \u0438\u043d\u0441\u0442\u0438\u0442\u0443\u0442\u044b \u04af\u0441\u0442\u0456\u043c\u0456\u0437\u0434\u0435\u0433\u0456 \u0436\u044b\u043b\u044b \u0411\u0456\u043b\u0456\u043c \u0436\u04d9\u043d\u0435 \u0493\u044b\u043b\u044b\u043c \u043c\u0438\u043d\u0438\u0441\u0442\u0440\u043b\u0456\u0433\u0456\u043d\u0456\u04a3 \u0492\u044b\u043b\u044b\u043c \u043a\u043e\u043c\u0438\u0442\u0435\u0442\u0456 \u0436\u0430\u0440\u0438\u044f\u043b\u0430\u0493\u0430\u043d 10 \u0433\u0440\u0430\u043d\u0442\u0442\u044b\u049b \u0436\u043e\u0431\u0430\u0493\u0430 \u0438\u0435 \u0431\u043e\u043b\u044b\u043f, \u0456\u0440\u0433\u0435\u043b\u0456 \u0437\u0435\u0440\u0442\u0442\u0435\u0443\u043b\u0435\u0440 \u0436\u04af\u0440\u0433\u0456\u0437\u0443 \u04af\u0441\u0442\u0456\u043d\u0434\u0435. \u0415\u043d\u0434\u0456 \u0441\u043e\u043b \u0436\u043e\u0431\u0430\u043b\u0430\u0440\u043c\u0435\u043d \u0441\u0456\u0437\u0434\u0435\u0440\u0434\u0456 \u0440\u0435\u0442-\u0440\u0435\u0442\u0456\u043c\u0435\u043d \u0442\u0430\u043d\u044b\u0441\u0442\u044b\u0440\u044b\u043f \u043e\u0442\u044b\u0440\u0430\u0442\u044b\u043d \u0431\u043e\u043b\u0430\u043c\u044b\u0437.\n\n\u2705 \u0410\u043b\u0493\u0430\u0448\u049b\u044b \u0436\u043e\u0431\u0430 \u0430\u0442\u0430\u0443\u044b: \u00ab\u04b0\u043b\u044b \u0434\u0430\u043b\u0430 \u043e\u0438\u0306\u044b\u043d\u0434\u0430\u0440\u044b\u00bb (\u041e\u0438\u0306\u044b\u043d\u0434\u044b\u049b \u0444\u043e\u043b\u044c\u043a\u043b\u043e\u0440): \u04b0\u043b\u0442\u0442\u044b\u049b \u043e\u0438\u0306\u044b\u043d\u0434\u0430\u0440\u0434\u044b\u04a3 \u0435\u0440\u0435\u043a\u0448\u0435 \u043c\u04d9\u0434\u0435\u043d\u0438 \u049b\u04b1\u0431\u044b\u043b\u044b\u0441 \u0440\u0435\u0442\u0456\u043d\u0434\u0435\u0433\u0456 \u049b\u043e\u0493\u0430\u043c\u0434\u044b\u049b \u0441\u0430\u043d\u0430\u043d\u044b \u0436\u0430\u04a3\u0493\u044b\u0440\u0442\u0443\u0434\u0430\u0493\u044b \u049b\u044b\u0437\u043c\u0435\u0442\u0456\u00bb\n\ud83d\udcdd\u0416\u043e\u0431\u0430 \u0436\u0435\u0442\u0435\u043a\u0448\u0456\u0441\u0456: \u041a.\u0406. \u041c\u0430\u0442\u044b\u0436\u0430\u043d\u043e\u0432, \u0444\u0438\u043b\u043e\u043b\u043e\u0433\u0438\u044f \u0493\u044b\u043b\u044b\u043c\u0434\u0430\u0440\u044b\u043d\u044b\u04a3 \u0434\u043e\u043a\u0442\u043e\u0440\u044b, \u049a\u0420 \u04b0\u0492\u0410-\u043d\u044b\u04a3 \u043a\u043e\u0440\u0440\u0435\u0441\u043f\u043e\u043d\u0434\u0435\u043d\u0442 \u043c\u04af\u0448\u0435\u0441\u0456\n\u2800\n\u0416\u043e\u0431\u0430 \u0430\u044f\u0441\u044b\u043d\u0434\u0430 \u0442\u04e9\u043c\u0435\u043d\u0434\u0435\u0433\u0456 \u043c\u0430\u04a3\u044b\u0437\u0434\u044b \u043c\u04d9\u0441\u0435\u043b\u0435\u043b\u0435\u0440 \u0437\u0435\u0440\u0442\u0442\u0435\u043b\u0435\u0434\u0456:\n\ud83d\udd3a\u043e\u0438\u0306\u044b\u043d \u0442\u0435\u043e\u0440\u0438\u044f\u0441\u044b \u0436\u04e9\u043d\u0456\u043d\u0434\u0435\u0433\u0456 \u04d9\u043b\u0435\u043c\u0434\u0456\u043a \u0493\u044b\u043b\u044b\u043c;\n\ud83d\udd3a\u043e\u0438\u0306\u044b\u043d\u043d\u044b\u04a3 \u0448\u044b\u0493\u0443 \u0442\u0435\u0433\u0456 \u0436\u04e9\u043d\u0456\u043d\u0434\u0435\u0433\u0456 \u043c\u0430\u0442\u0435\u0440\u0438\u0430\u043b\u0438\u0441\u0442\u0456\u043a \u0442\u0435\u043e\u0440\u0438\u044f \u043c\u0435\u043d \u0438\u0434\u0435\u044f\u043b\u044b\u049b \u04b1\u0441\u0442\u0430\u043d\u044b\u043c\u0434\u0430\u0440 \u0442\u0430\u0440\u0438\u0445\u043d\u0430\u043c\u0430\u0441\u044b;\n\ud83d\udd3a\u043e\u0438\u0306\u044b\u043d \u043c\u0435\u043d \u043c\u04d9\u0434\u0435\u043d\u0438\u0435\u0442\u0442\u0456\u04a3 \u0430\u0440\u0430\u049b\u0430\u0442\u044b\u043d\u0430\u0441\u044b, \u043e\u043d\u044b\u04a3 \u0430\u0434\u0430\u043c\u0437\u0430\u0442 \u04e9\u0440\u043a\u0435\u043d\u0438\u0435\u0442\u0456\u043d \u0434\u0430\u043c\u044b\u0442\u0443\u0434\u0430\u0493\u044b \u0440\u04e9\u043b\u0456;\n\ud83d\udd3a\u043e\u0438\u0306\u044b\u043d\u043d\u044b\u04a3 \u049b\u0430\u043b\u044b\u043f\u0442\u0430\u0441\u0443\u044b\u043d\u0434\u0430\u0493\u044b \u0442\u0430\u0440\u0438\u0445 \u043f\u0435\u043d \u0445\u0430\u043b\u044b\u049b\u0442\u044b\u04a3 \u04e9\u043c\u0456\u0440 \u0441\u0430\u043b\u0442\u044b\u043d\u044b\u04a3 \u043c\u0430\u04a3\u044b\u0437\u044b;\n\ud83d\udd3a\u049b\u0430\u0437\u0430\u049b \u0445\u0430\u043b\u044b\u049b \u043e\u0438\u0306\u044b\u043d\u0434\u0430\u0440\u044b\u043d\u044b\u04a3 \u0436\u0438\u043d\u0430\u043b\u0443 \u0442\u0430\u0440\u0438\u0445\u043d\u0430\u043c\u0430\u0441\u044b;\n\ud83d\udd3a\u0445\u0430\u043b\u044b\u049b \u043e\u0438\u0306\u044b\u043d\u0434\u0430\u0440\u044b\u043d\u044b\u04a3 \u0442\u0430\u0440\u0438\u0445\u0438 \u0434\u0435\u0440\u0435\u043a\u0442\u0435\u0440 \u043c\u0435\u043d \u043a\u04e9\u043d\u0435 \u0442\u04af\u0440\u043a\u0456 \u0436\u0430\u0437\u0431\u0430 \u0435\u0441\u043a\u0435\u0440\u0442\u043a\u0456\u0448\u0442\u0435\u0440\u0456\u043d\u0434\u0435\u0433\u0456, \u0444\u043e\u043b\u044c\u043a\u043b\u043e\u0440\u043b\u044b\u049b \u0448\u044b\u0493\u0430\u0440\u043c\u0430\u043b\u0430\u0440\u0434\u0430\u0493\u044b  \u043a\u04e9\u0440\u0456\u043d\u0456\u0441\u0456;\n\ud83d\udd3a\u049b\u0430\u0437\u0430\u049b\u0442\u044b\u04a3 \u04b1\u043b\u0442\u0442\u044b\u049b \u043e\u0438\u0306\u044b\u043d\u0434\u0430\u0440\u044b\u043d\u044b\u04a3  \u0445\u0430\u0442\u049b\u0430 \u0442\u04af\u0441\u0443\u0456, \u04b1\u043b\u0442\u0442\u044b\u049b \u043e\u0438\u0306\u044b\u043d\u0434\u0430\u0440\u0434\u044b\u04a3 \u0436\u0430\u043b\u043f\u044b \u0441\u0438\u043f\u0430\u0442\u044b; \u043e\u043d\u044b\u04a3 \u0442\u0430\u0440\u0438\u0445\u0438-\u044d\u0442\u043d\u043e\u0433\u0440\u0430\u0444\u0438\u044f\u043b\u044b\u049b \u0442\u04b1\u0440\u0493\u044b\u0434\u0430\u043d \u0441\u0430\u0440\u0430\u043b\u0430\u043d\u0443 \u0442\u0430\u0440\u0438\u0445\u043d\u0430\u043c\u0430\u0441\u044b;\n\ud83d\udd3a\u0493\u04b1\u0440\u044b\u043f\u0442\u044b\u049b, \u0441\u043f\u043e\u0440\u0442\u0442\u044b\u049b, \u043e\u0438\u0306\u044b\u043d-\u0441\u0430\u0443\u044b\u049b\u0442\u044b\u049b \u043e\u0438\u0306\u044b\u043d\u0434\u0430\u0440\u0434\u044b\u04a3 \u0431\u04af\u0433\u0456\u043d\u0433\u0456 \u04b1\u043b\u0442\u0442\u044b\u049b \u043c\u04d9\u0434\u0435\u043d\u0438\u0435\u0442\u0442\u0435\u0433\u0456 \u043e\u0440\u043d\u044b;\n\ud83d\udd3a\u04b1\u043b\u0442\u0442\u044b\u049b \u043e\u0438\u0306\u044b\u043d\u0434\u0430\u0440\u0434\u044b\u04a3 \u0431\u04af\u0433\u0456\u043d\u0433\u0456 \u043a\u04af\u043d\u0434\u0435\u0433\u0456 \u0430\u0445\u0443\u0430\u043b\u044b, \u043e\u043d\u044b \u0434\u0430\u043c\u044b\u0442\u0443\u0434\u044b\u04a3, \u0436\u0430\u04a3\u0430 \u04e9\u043c\u0456\u0440\u0433\u0435 \u043a\u0456\u0440\u0456\u043a\u0442\u0456\u0440\u0443\u0434\u0456\u04a3 \u04e9\u0437\u0435\u043a\u0442\u0456 \u043c\u04d9\u0441\u0435\u043b\u0435\u043b\u0435\u0440\u0456.\n\u2800\n\u2705\u0416\u043e\u0431\u0430\u043d\u044b \u0436\u04af\u0437\u0435\u0433\u0435 \u0430\u0441\u044b\u0440\u0443 \u0431\u0430\u0440\u044b\u0441\u044b\u043d\u0434\u0430 \u0411\u0492\u0421\u0411\u041a \u04b1\u0441\u044b\u043d\u0430\u0442\u044b\u043d \u0436\u0443\u0440\u043d\u0430\u043b\u0434\u0430\u0440\u0434\u0430 6 \u043c\u0430\u049b\u0430\u043b\u0430, \u0448\u0435\u0442\u0435\u043b\u0434\u0456\u043a \u0440\u0435\u0446\u0435\u043d\u0437\u0438\u044f\u043b\u0430\u043d\u0430\u0442\u044b\u043d \u0436\u0443\u0440\u043d\u0430\u043b\u0434\u0430\u0440\u0434\u0430 2 \u043c\u0430\u049b\u0430\u043b\u0430 \u0436\u0430\u0440\u0438\u044f\u043b\u0430\u043d\u0430\u0434\u044b. \u0417\u0435\u0440\u0442\u0442\u0435\u0443 \u0436\u04b1\u043c\u044b\u0441\u044b\u043d\u044b\u04a3 \u0430\u049b\u0442\u044b\u049b \u043d\u04d9\u0442\u0438\u0436\u0435\u0441\u0456 \u0440\u0435\u0442\u0456\u043d\u0434\u0435 \u00ab\u04b0\u043b\u044b \u0434\u0430\u043b\u0430 \u043e\u0438\u0306\u044b\u043d\u0434\u0430\u0440\u044b\u00bb \u0430\u0442\u0442\u044b \u0436\u0438\u043d\u0430\u049b \u043f\u0435\u043d \u00ab\u04b0\u043b\u044b \u0434\u0430\u043b\u0430 \u043e\u0438\u0306\u044b\u043d\